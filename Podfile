  pod 'ReachabilitySwift'
  pod 'TrueTime', git: 'https://github.com/instacart/TrueTime.swift'
  target 'DinnerSystemTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DinnerSystemUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
